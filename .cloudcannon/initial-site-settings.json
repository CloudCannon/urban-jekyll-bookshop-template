{
  "ssg": "jekyll",
  "build_configuration": {
    "preserved_paths": "",
    "source": "site",
    "environment_variables": [
        { "key": "BUNDLE_GEMFILE", "value": "site/Gemfile" }
      ],
    "jekyll_environment": "production",
    "drafts": false,
    "use_local_bundle": true,
    "enable_bundle_cache": true
  }
}